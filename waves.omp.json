{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 2,
  "final_space": true,
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "p:white",
          "background": "transparent",
          "template": "\u250f "
        },
        {
          "type": "os",
          "style": "powerline",
          "foreground": "#00c983",
          "background": "#003543",
          "trailing_diamond": " ",
          "template": " {{ .Icon }} "
        },
        {
          "type": "path",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#ffffff",
          "background": "#94da62",
          "template": " <#000>\uf07b \uf553</> {{ .Path }} ",
          "properties": {
            "folder_icon": "\uf115",
            "folder_separator_icon": "\\",
            "home_icon": "\uf7db",
            "style": "full"
          }
        },
        {
          "type": "time",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#ffffff",
          "background": "#275773",
          "template": " \uf017 {{ .CurrentDate | date .Format }} ",
          "properties": {
            "time_format": "3:04 PM"
          }
        },
        {
          "type": "executiontime",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#ffffff",
          "background": "#815ec3",
          "template": " \ufbab {{ .FormattedMs }} ",
          "properties": {
            "always_enabled": true
          }
        },
        {
          "type": "exit",
          "style": "diamond",
          "powerline_symbol": "\ue0b0",
          "foreground": "#ffffff",
          "background": "#2e9599",
          "background_templates": [
            "{{ if gt .Code 0 }}red{{ end }}"
          ],
          "trailing_diamond": "\ue0b4",
          "template": " {{ if gt .Code 0 }}\uf525{{ else }}\uf469{{ end }} ",
          "properties": {
            "always_enabled": true
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "type": "root",
          "style": "plain",
          "foreground": "#cd5e42",
          "template": "\ue3bf "
        },
        {
          "type": "git",
          "style": "powerline",
          "foreground": "#ffffff",
          "background": "#2d333b",
          "template": "<#ffffff> âžœ ({{ .UpstreamIcon }}:{{ .HEAD }}{{ if gt .StashCount 0 }} \uf692 {{ .StashCount }}{{ end }}) ",
          "properties": {
            "branch_icon": " <#ffffff>\ue0a0 </>",
            "fetch_stash_count": true,
            "fetch_status": false,
            "fetch_upstream_icon": true
          }
        },
        {
          "type": "node",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#ffffff",
          "background": "#83d26f",
          "template": "<#FFFFFF> \ue718 v{{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} "
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "root",
          "style": "plain",
          "foreground": "#cd5e42",
          "template": "\ue3bf "
        },
        {
          "type": "text",
          "style": "plain",
          "foreground": "#b4cd42",
          "template": "<#FFFFFF>\u2517 <#45F1C2> {{ .UserName }}@{{ .HostName }}</b> <#FFFFFF>~"
        }
      ],
      "newline": true
    }
  ]
}
